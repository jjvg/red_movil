<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.redcom" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>RedCom</name>
    <description>
      Red Social Comunitaria
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
      Equipo Lab
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="src/assets/img/rc1.png" />
    <icon density="mdpi" src="src/assets/img/rc1.png" />
    <icon density="hdpi" src="src/assets/img/rc1.png" />
    <icon density="xhdpi" src="src/assets/img/rc1.png" />
    <splash density="land-hdpi" src="src/assets/img/splash1.png" />
    <splash density="land-ldpi" src="src/assets/img/splash1.png" />
    <splash density="land-mdpi" src="src/assets/img/splash1.png" />
    <splash density="land-xhdpi" src="src/assets/img/splash1.png" />
    <splash density="port-hdpi" src="src/assets/img/splash1.png" />
    <splash density="port-ldpi" src="src/assets/img/splash1.png" />
    <splash density="port-mdpi" src="src/assets/img/splash1.png" />
    <splash density="port-xhdpi" src="src/assets/img/splash1.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
